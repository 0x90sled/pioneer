model(hermite_wank) {
	# 1/8th sphere
	v6 = v(0.0, 5.0, 0.0)
	v7 = v(-5.0, 0.0, 0.0 )
	v8 = v(0.0, 0.0, 5.0 )

	v9 = v(-7.0, 0.0, 0.0 )			
	v10 = v(0.0, 7.0, 0.0 )
	v11 = v(0.0, 0.0, 7.0 )			
	xref:smooth(8, v6,
		hermite(v7, v9, v10),
		hermite(v8, v11, v9),
		hermite(v6, v10, v11))
}

model(ship1) {
	xref:tube(16, v(15,-20,0), animlin(v(15,-15,0), v(15,-10,0)), v(0,0,1), innerrad=5.0, outerrad=10.0)
	quad(v(1,-2,0), v(1,-1,0), v(0,-1,0), v(0,-2,0))
	circle(16, v(15,0,0), v(0,0,1), v(0,1,0), radius=10.0)
	xref:cylinder(16, v(15,10,0), v(15,20,0), v(0,0,1), radius=10.0)
	subobject(hermite_wank, v(0,0,0), v(0,1,0), v(0,0,1), scale=1.0)
	subobject(hermite_wank, v(0,0,0), v(0,-1,0), v(0,0,1), scale=1.0, anim=0)
}

